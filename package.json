{
  "name": "@dj-ui/source",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.14.4",
  "scripts": {
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "release-all": "nx release --skip-publish",
    "publish-all": "nx release publish",
    "check-circular-dep": "madge --circular --exclude 'dist' --extensions ts ./apps ./libs",
    "lint:affected": "pnpm nx affected -t lint",
    "stylelint:affected": "pnpm nx affected -t stylelint",
    "lint:all": "pnpm nx run-many --all --skip-nx-cache --parallel=12 -t lint",
    "stylelint:all": "pnpm nx run-many --all --skip-nx-cache --parallel=12 -t stylelint",
    "fix-lint": "pnpm nx affected -t lint --fix",
    "type-check:affected": "pnpm nx affected -t typecheck",
    "typecheck:all": "pnpm nx run-many --all --skip-nx-cache --parallel=12 -t typecheck",
    "build:affected": "pnpm nx affected -t build",
    "build:all": "pnpm nx run-many --all --skip-nx-cache --parallel=12 -t build",
    "e2e:affected": "pnpm nx affected -t e2e",
    "e2e:all": "pnpm nx run-many --all --skip-nx-cache --parallel=12 -t e2e"
  },
  "dependencies": {
    "@angular/animations": "~18.2.0",
    "@angular/common": "~18.2.0",
    "@angular/compiler": "~18.2.0",
