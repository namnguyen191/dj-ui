    "lint-staged": "16.1.2 ",
    "madge": "8.0.0",
    "ng-packagr": "20.1.0",
    "nx": "21.2.3",
    "nx-stylelint": "18.0.0",
    "postcss": "8.5.3",
    "postcss-url": "10.1.3",
    "prettier": "3.5.3",
    "stylelint": "16.17.0",
    "stylelint-config-standard": "37.0.0",
    "stylelint-config-standard-scss": "14.0.0",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "tsx": "4.19.3",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.33.1",
    "typescript-json-schema": "^0.65.1",
    "vite": "6.2.3",
    "vitest": "3.1.1"
  },
  "pnpm": {
    "overrides": {
      "flatpickr": "4.6.13",
      "less": "^4.2.1"
    }
  }
}
