    "carbon-components": "^10.58.15",
    "carbon-components-angular": "^5.56.4",
    "lodash-es": "^4.17.21",
    "monaco-editor": "^0.50.0",
    "ngrx-immer": "^3.0.0",
    "ngx-monaco-editor-v2": "^18.1.0",
    "rxjs": "~7.8.0",
    "type-fest": "^4.29.0",
    "zod": "^3.23.8",
