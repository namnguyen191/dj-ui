{
  "name": "@dj-ui/source",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.14.4",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm",
    "start": "nx run prime-ng-playground:serve",
    "release-all": "nx release --skip-publish",
    "publish-all": "nx release publish",
    "check-circular-dep": "madge --circular --exclude 'dist' --extensions ts ./apps ./libs",
    "lint:affected": "nx affected -t lint",
    "stylelint:affected": "nx affected -t stylelint",
    "lint:all": "nx run-many --all --skip-nx-cache --parallel=12 -t lint",
    "stylelint:all": "nx run-many --all --skip-nx-cache --parallel=12 -t stylelint",
    "fix-lint": "nx affected -t lint --fix",
    "type-check:affected": "nx affected -t typecheck",
    "typecheck:all": "nx run-many --all --skip-nx-cache --parallel=12 -t typecheck",
    "build:affected": "nx affected -t build",
    "build:all": "nx run-many --all --skip-nx-cache --parallel=12 -t build",
    "test:affected": "nx affected -t test",
    "test:all": "nx run-many --all --skip-nx-cache --parallel=12 -t test",
    "e2e:affected": "nx affected -t e2e",
    "e2e:all": "nx run-many --all --skip-nx-cache --parallel=12 -t e2e",
    "generate-json-schema:all": "nx run-many --all --skip-nx-cache --parallel=12 -t generate-json-schema-with-zod",
    "clean-artifacts": "nx reset && rm -rf .nx .angular dist tmp node_modules/.cache",
    "fullcheck": "pnpm lint:all && pnpm stylelint:all && pnpm typecheck:all && pnpm test:all"
  },
  "dependencies": {
    "@angular/animations": "20.1.0",
    "@angular/common": "20.1.0",
    "@angular/compiler": "20.1.0",
    "@angular/core": "20.1.0",
    "@angular/forms": "20.1.0",
    "@angular/platform-browser": "20.1.0",
    "@angular/platform-browser-dynamic": "20.1.0",
    "@angular/router": "20.1.0",
    "@carbon/icons": "^11.53.0",
    "@carbon/styles": "^1.70.0",
    "@monaco-editor/loader": "^1.5.0",
    "@namnguyen191/common-angular-helper": "1.1.2",
    "@namnguyen191/common-js-helper": "1.1.3",
    "@ngrx/signals": "19.2.1",
    "@primeng/themes": "19.1.3",
    "carbon-components": "^10.58.15",
    "carbon-components-angular": "^5.56.4",
    "lodash-es": "^4.17.21",
    "monaco-editor": "0.52.2",
    "ngrx-immer": "^3.0.0",
    "ngx-monaco-editor-v2": "^19.0.2",
    "primeicons": "^7.0.0",
    "primeng": "19.1.3",
    "rxjs": "7.8.2",
    "type-fest": "4.41.0",
    "zod": "4.0.5"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "1.19.1",
    "@analogjs/vitest-angular": "1.19.1",
    "@angular-devkit/architect": "0.2001.0",
    "@angular-devkit/build-angular": "20.1.0",
    "@angular-devkit/core": "20.1.0",
    "@angular-devkit/schematics": "20.1.0",
    "@angular/cli": "20.1.0",
    "@angular/compiler-cli": "20.1.0",
    "@angular/language-service": "20.1.0",
    "@eslint/js": "9.31.0",
    "@nx/angular": "21.2.3",
    "@nx/devkit": "21.2.3",
    "@nx/eslint": "21.2.3",
    "@nx/eslint-plugin": "21.2.3",
    "@nx/js": "21.2.3",
    "@nx/playwright": "21.2.3",
    "@nx/vite": "21.2.3",
    "@nx/web": "21.2.3",
    "@nx/workspace": "21.2.3",
    "@playwright/test": "1.51.1",
    "@schematics/angular": "20.1.0",
    "@swc-node/register": "1.10.10",
    "@swc/core": "1.11.13",
    "@swc/helpers": "0.5.15",
    "@types/lodash-es": "4.17.12",
    "@types/node": "22.13.14",
    "@typescript-eslint/utils": "^8.33.1",
    "@vitest/coverage-v8": "3.1.1",
    "@vitest/ui": " 3.1.1",
    "angular-eslint": "20.1.1",
    "autoprefixer": "10.4.21",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-playwright": "2.2.0",
    "eslint-plugin-prettier": "5.2.5",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "husky": "9.1.7",
    "jiti": "2.4.2",
    "jsdom": "26.1.0",
